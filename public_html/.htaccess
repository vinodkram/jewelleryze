#Protect .htaccess and .htpwd file start
<Files ~ "^.*\.([Hh][Tt][Aa])">
order allow,deny
deny from all
satisfy all
</Files>
<Files ~ "^.*\.([Hh][Tt][Pp])">
order allow,deny
deny from all
satisfy all
</Files>
#Protect .htaccess and .htpwd file end
<IfModule mod_rewrite.c>
<IfModule mod_negotiation.c>
Options -MultiViews
</IfModule>
RewriteEngine On
##### Code added to force https with www start #####

#RewriteCond %{HTTP:X-Forwarded-Proto} !https
